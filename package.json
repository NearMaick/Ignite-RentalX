    "dockerBuild": "docker build -t rentx .",
    "dockerRun": "docker run -p 3333:3333 rentx",
    "dockerCompose": "docker-compose up --force-recreate",
    "dockerStart": "docker-compose start",
    "test": "cross-env NODE_ENV=test jest --watchAll --runInBand --detectOpenHandles",
